{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.profiler", {profile: token}, {fragment: 'service'}) }}" title="{{ 'Service'|t }}">
        <img width="20" height="28" alt="State"
             src="data:image/png;base64,"/>
        <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.initializedServicesCount }}</span>
    </a>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>{{ 'Initialized'|t }}</b>
        <span>{{ collector.initializedServicesCount }} ({{ collector.initializedServicesWithoutWebprofilerCount }})</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>{{ 'Available'|t }}</b>
        <span>{{ collector.servicesCount }}</span>
    </div>
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block panel %}
    <div class="title">
        <h2>Service</h2>
    </div>

    <div class="content">
        {{ content }}
    </div>
{% endblock %}
