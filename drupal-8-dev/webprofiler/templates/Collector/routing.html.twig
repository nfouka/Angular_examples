{% block toolbar %}
  {% set icon %}
  <a href="{{ url("webprofiler.profiler", {profile: token}, {fragment: 'routing'}) }}" title="{{ 'Routing'|t }}">
    <img width="20" height="28" alt="Asset"
         src="data:image/png;base64,"
         alt="Routing">
    <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.routingCount }}</span>
  </a>

  {% endset %}

  {% set text %}
  <div class="sf-toolbar-info-piece">
    <b>{{ 'Routes'|t }}</b>
    <span>{{ collector.routingCount }}</span>
  </div>
  {% endset %}

  <div class="sf-toolbar-block">
    <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
    <div class="sf-toolbar-info">{{ text|default('') }}</div>
  </div>
{% endblock %}

{% block panel %}
    <div class="title">
        <h2>{{ 'Routing'|t }}</h2>
    </div>

    <div class="content">
        {{ content }}
    </div>
{% endblock %}
