
    <link rel="stylesheet" href="http://localhost/drupal-7.36/sites/all/modules/acme/src/css/style.css">
<script type="text/javascript" src="http://dciss.org/projet/d-8-angular-json-tableSorter/templates/js/angular-tablesort.js"></script> 
      
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.0.4/angular.min.js"></script>
  <script src="http://localhost/drupal-7.36/sites/all/modules/acme/src/js/repos.js"></script>

<style type="text/css">

</style>

  </head>

  <body>

   <div class="container" ng-controller="GitHubReposController" ng-app="myApp">
   
      <h1>GitHub Repos</h1>

      
      <table border="1" ts-wrapper>  
  <tr>
                
                <th >NAME </th>
                <th >JOB</th>
                 <th>COUNTRY</th>
                <th >PARTENER</th>
                <th >SIRET</th>
                <th >ADRESS</th>
               
  </tr>
  <tr ng-repeat="x in repos" ts-repeat>
   
    <td>  {{x.name || "Not specified"}} </td>
    <td>  {{x.job || "Not specified"}} </td>
    <td>  {{x.country || "Not specified"}} </td>
    <td>  {{x.partener || "Not specified"}} </td>
    <td>  {{x.siret || "Not specified"}} </td>
    <td>  {{x.adress || "Not specified"}} </td>
    
    
  </tr>
      </table>
      </div>
