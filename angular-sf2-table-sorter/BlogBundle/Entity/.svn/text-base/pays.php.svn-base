namespace App\BlogBundle\Entity;

/**
 * Description of pays
 *
 * @author nadir
 */
class pays {
    
    private $nom_fr_fr ; 
    private $nom_en_gb ; 
    
    
    function __construct($nom_fr_fr, $nom_en_gb) {
        $this->nom_fr_fr = $nom_fr_fr;
        $this->nom_en_gb = $nom_en_gb;
    }

    function getNom_fr_fr() {
        return $this->nom_fr_fr;
    }

    function getNom_en_gb() {
        return $this->nom_en_gb;
    }

    function setNom_fr_fr($nom_fr_fr) {
        $this->nom_fr_fr = $nom_fr_fr;
    }

    function setNom_en_gb($nom_en_gb) {
        $this->nom_en_gb = $nom_en_gb;
    }

}
